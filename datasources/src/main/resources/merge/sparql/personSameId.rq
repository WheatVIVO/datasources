PREFIX swrl:     <http://www.w3.org/2003/11/swrl#>
PREFIX swrlb:    <http://www.w3.org/2003/11/swrlb#>
PREFIX vitro:    <http://vitro.mannlib.cornell.edu/ns/vitro/0.7#>
PREFIX p1:       <http://vivo.wheatinitiative.org/ontology/adminapp/>
PREFIX bibo:     <http://purl.org/ontology/bibo/>
PREFIX c4o:      <http://purl.org/spar/c4o/>
PREFIX cito:     <http://purl.org/spar/cito/>
PREFIX dcterms:  <http://purl.org/dc/terms/>
PREFIX event:    <http://purl.org/NET/c4dm/event.owl#>
PREFIX fabio:    <http://purl.org/spar/fabio/>
PREFIX foaf:     <http://xmlns.com/foaf/0.1/>
PREFIX geo:      <http://aims.fao.org/aos/geopolitical.owl#>
PREFIX obo:      <http://purl.obolibrary.org/obo/>
PREFIX ocrer:    <http://purl.org/net/OCRe/research.owl#>
PREFIX ocresst:  <http://purl.org/net/OCRe/statistics.owl#>
PREFIX ocresd:   <http://purl.org/net/OCRe/study_design.owl#>
PREFIX ocresp:   <http://purl.org/net/OCRe/study_protocol.owl#>
PREFIX ro:       <http://purl.obolibrary.org/obo/ro.owl#>
PREFIX skos:     <http://www.w3.org/2004/02/skos/core#>
PREFIX swo:      <http://www.ebi.ac.uk/efo/swo/>
PREFIX vcard:    <http://www.w3.org/2006/vcard/ns#>
PREFIX vitro-public: <http://vitro.mannlib.cornell.edu/ns/vitro/public#>
PREFIX vivo:     <http://vivoweb.org/ontology/core#>
PREFIX scires:   <http://vivoweb.org/ontology/scientific-research#>
PREFIX vann:     <http://purl.org/vocab/vann/>

SELECT ?value ?g ?definitive ?alreadyMatched ?cienciaId (STR(?orcidUri) AS ?orcidId) ?researcherId ?scopusId ?x WHERE {
  {
    SELECT DISTINCT ?value WHERE {
      ?x ?idp ?value
    } ORDER BY ?value LIMIT ?limit OFFSET ?
  }
  GRAPH ?g { ?x ?idp ?value }
  BIND(IF(REGEX(STR(?x), ?value), "true", "false") AS ?definitive)
  OPTIONAL {
    ?x owl:sameAs ?something.
    FILTER(?something != ?x)
    BIND("true" AS ?alreadyMatched)
  }
  OPTIONAL { ?x <https://vivo.uminho.pt/ontology/cienciavitae/ciencia-id> ?cienciaId }
  OPTIONAL { ?x vivo:orcidId ?orcidUri }
  OPTIONAL { ?x vivo:researcherId ?researcherId }
  OPTIONAL { ?x vivo:scopusId ?scopusId }
} ORDER BY ?value ?g DESC(?definitive) DESC(?alreadyMatched) DESC(?cienciaId) DESC(?orcidUri) DESC(?researcherId) DESC(?scopusId) ?x
